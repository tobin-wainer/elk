	keywords = {methods: data analysis, methods: statistical, Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Solar and Stellar Astrophysics, stars: variables: other},
	pages = {1897--1910},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/KAI6JLYQ/Shin et al. - 2009 - Detecting variability in massive astronomical time.pdf:application/pdf},
}

