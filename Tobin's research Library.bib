	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: star formation, Astrophysics - Solar and Stellar Astrophysics, stars: rotation, asteroseismology, astronimical data bases, miscellaneous, software: data analysis, stars: variables: Scuti},
	pages = {374--388},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/4KDTS98S/Pamos Ortega et al. - 2022 - Determining the seismic age of the young open clus.pdf:application/pdf},
}

