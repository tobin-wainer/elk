@article{alonso-garcia_variable_2021,
	title = {Variable stars in the {VVV} globular clusters. {II}. {NGC} 6441, {NGC} 6569, {NGC} 6626 ({M} 28), {NGC} 6656 ({M} 22), {2MASS}-{GC} 02, and {Terzan} 10},
