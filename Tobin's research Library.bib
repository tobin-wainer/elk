	note = {ADS Bibcode: 2009ApJ...704.1120U},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: individual: M33, stars: early-type, Astrophysics - Cosmology and Nongalactic Astrophysics, stars: abundances, galaxies: distances and redshifts, supergiants},
	pages = {1120--1134},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/3PZVVZ2S/U et al. - 2009 - A New Distance to M33 Using Blue Supergiants and t.pdf:application/pdf},
