	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Guillot, Sebastien and Rutledge, Robert E. and Bildsten, Lars and Brown, Edward F. and Pavlov, George G. and Zavlin, Vyacheslav E.},
