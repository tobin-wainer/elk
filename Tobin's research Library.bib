	journal = {Astronomy and Astrophysics},
	author = {Alonso-García, Javier and Smith, Leigh C. and Catelan, Márcio and Minniti, Dante and Navarrete, Camila and Borissova, Jura and Carballo-Bello, Julio A. and Contreras Ramos, Rodrigo and Fernández-Trincado, José G. and Ferreira Lopes, Carlos E. and Gran, Felipe and Garro, Elisa R. and Geisler, Doug and Guo, Zhen and Hempel, Maren and Kerins, Eamonn and Lucas, Philip W. and Palma, Tali and Peña Ramírez, Karla and Ramírez Alegría, Sebastián and Saito, Roberto K.},
	month = jul,
