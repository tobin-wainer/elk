	author = {Castro-Ginard, A. and Jordi, C. and Luri, X. and Cantat-Gaudin, T. and Carrasco, J. M. and Casamiquela, L. and Anders, F. and Balaguer-Núñez, L. and Badia, R. M.},
	month = may,
	year = {2022},
	note = {ADS Bibcode: 2022A\&A...661A.118C},
	keywords = {astrometry, Astrophysics - Astrophysics of Galaxies, Galaxy: disk, methods: data analysis, open clusters and associations: general},
	pages = {A118},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/YZS2W2HB/Castro-Ginard et al. - 2022 - Hunting for open clusters in Gaia EDR3 628 new op.pdf:application/pdf},
}

