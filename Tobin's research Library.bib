	keywords = {Magellanic Clouds, Astrophysics - Solar and Stellar Astrophysics, Stars: variables: Cepheids, Stars: oscillations, Stars: Population II},
	pages = {91--107},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/JL3MDGJF/Soszyński et al. - 2010 - The Optical Gravitational Lensing Experiment. The .pdf:application/pdf},
