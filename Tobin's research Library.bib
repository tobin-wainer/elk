@article{de_meulenaer_deriving_2013,
	title = {Deriving physical parameters of unresolved star clusters. {I}. {Age}, mass, and extinction degeneracies},
	volume = {550},
	url = {https://ui.adsabs.harvard.edu/abs/2013A%26A...550A..20D/abstract},
	doi = {10.1051/0004-6361/201220674},
	abstract = {Context. Stochasticity and physical parameter degeneracy problems complicate the derivation of the parameters (age, mass, and extinction) of unresolved star clusters when using broad-band photometry. {\textless}BR /{\textgreater} Aims: We develop a method to simulate stochasticity and degeneracies and to investigate their influence on the accuracy of derived physical parameters. Then we apply it to star cluster samples of M 31 and M 33 galaxies. {\textless}BR /{\textgreater} Methods: Age, mass, and extinction of observed star clusters are derived by comparing their broad-band UBVRI integrated magnitudes to the magnitudes of a large grid of star cluster models with fixed metallicity Z = 0.008. Masses of stars for a cluster model are randomly sampled from the initial mass function. Models of star clusters from the model grid, which have all of their magnitudes located within 3 observational errors from the magnitudes of the observed cluster, are selected for computing their age, mass, and extinction. {\textless}BR /{\textgreater} Results: In the case of the M 31 galaxy, the extinction range is wide and the age-extinction degeneracy is strong for a fraction of its clusters. Because of a narrower extinction range, the age-extinction degeneracy is weaker for the M 33 clusters. By using artificial cluster sample, we show that age-extinction degeneracy can be reduced significantly if the range of intrinsic extinction within the host galaxy is narrow. Figures 6-8 are available in electronic form at {\textless}A href="http://www.aanda.org"{\textgreater}http://www.aanda.org{\textless}/A{\textgreater}},
	language = {en},
	urldate = {2019-07-18},
	journal = {Astronomy and Astrophysics},
	author = {de Meulenaer, P. and Narbutis, D. and Mineikis, T. and Vansevičius, V.},
	month = feb,
	year = {2013},
	pages = {A20},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/Z2V9DUWP/de Meulenaer et al. - 2013 - Deriving physical parameters of unresolved star cl.pdf:application/pdf;Snapshot:/Users/Tobin/Zotero/storage/AGPF4UZV/abstract.html:text/html},
}

@article{de_meulenaer_deriving_2014,
	title = {Deriving physical parameters of unresolved star clusters. {II}. {The} degeneracies of age, mass, extinction, and metallicity},
	volume = {569},
	url = {https://ui.adsabs.harvard.edu/abs/2014A%26A...569A...4D/abstract},
	doi = {10.1051/0004-6361/201423988},
	abstract = {Context. This paper is the second of a series that investigates the stochasticity and degeneracy problems that hinder the derivation of the age, mass, extinction, and metallicity of unresolved star clusters in external galaxies when broad-band photometry is used. {\textless}BR /{\textgreater} Aims: While Paper I concentrated on deriving age, mass, and extinction of star clusters for one fixed metallicity, we here derive these parameters in case when metallicity is let free to vary. The results were obtained using several different filter systems (UBVRI, UBVRIJHK, GALEX+UBVRI), which allowed us to optimally reduce the different degeneracies between the cluster physical parameters. {\textless}BR /{\textgreater} Methods: The age, mass, and extinction of a sample of artificial star clusters were derived by comparing their broad-band integrated magnitudes with the magnitudes of a large grid of cluster models with various metallicities. {\textless}BR /{\textgreater} Results: A large collection of artificial clusters was studied to model the different degeneracies in the age, mass, extinction, and metallicity parameter space when stochasticity is taken into account in the cluster models. We show that, without prior knowledge on the metallicity, the optical bands (UBVRI) fail to allow a correct derivation of the age, mass, and extinction because of the strong degeneracies between models of different metallicities. Adding near-infrared information (UBVRI+JHK) slightly helps in improving the parameter derivation, except for the metallicity. Adding ultraviolet data (GALEX+UBVRI) helps significantly in deriving these parameters and allows constraining the metallicity when the photometric errors have a Gaussian distribution with standard deviations 0.05 mag for UBVRI and 0.15 mag for the GALEX bands.},
	language = {en},
	urldate = {2019-07-18},
	journal = {Astronomy and Astrophysics},
	author = {de Meulenaer, P. and Narbutis, D. and Mineikis, T. and Vansevičius, V.},
	month = sep,
	year = {2014},
	pages = {A4},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/JE2W9KX3/de Meulenaer et al. - 2014 - Deriving physical parameters of unresolved star cl.pdf:application/pdf;Snapshot:/Users/Tobin/Zotero/storage/GPEG27ET/abstract.html:text/html},
}

@article{de_meulenaer_deriving_2015,
