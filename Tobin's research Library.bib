	note = {arXiv:2208.11721 [astro-ph]},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics},
	file = {arXiv Fulltext PDF:/Users/Tobin/Zotero/storage/UVMF3SN7/Fetherolf et al. - 2022 - Variability Catalog of Stars Observed During the T.pdf:application/pdf;arXiv.org Snapshot:/Users/Tobin/Zotero/storage/DRQSWC3V/2208.html:text/html},
