	keywords = {galaxies: star clusters, galaxies: individual: M33, galaxies: individual: M51, Magellanic Clouds, open clusters and associations: general, solar neighbourhood},
	pages = {717--732},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/V6JRFGU5/Boutloukos and Lamers - 2003 - Star cluster formation and disruption time-scales .pdf:application/pdf},
