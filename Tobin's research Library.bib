	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics, 73, Asteroseismology, 1595, 686, 983, Ground-based astronomy, M giant stars, Stellar distance},
	pages = {18},
