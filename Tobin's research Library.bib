	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: spiral, galaxies: evolution, galaxies: kinematics and dynamics, dark matter},
	pages = {1909--1930},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/7N4399WC/Aniyan et al. - 2018 - Resolving the disc-halo degeneracy - I a look at .pdf:application/pdf},
}

