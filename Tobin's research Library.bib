@article{wainer_panchromatic_2022,
	title = {The {Panchromatic} {Hubble} {Andromeda} {Treasury}: {Triangulum} {Extended} {Region} ({PHATTER}). {III}. {The} {Mass} {Function} of {Young} {Stellar} {Clusters} in {M33}},
