	note = {ADS Bibcode: 2021MNRAS.505.1342M},
	keywords = {Astrophysics - Astrophysics of Galaxies, open clusters and associations: general, methods: data analysis, stars: kinematics and dynamics, catalogues, stars: variables: Cepheids},
	pages = {1342--1366},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/EWZNFH44/Medina et al. - 2021 - A revisited study of Cepheids in open clusters in .pdf:application/pdf},
}

