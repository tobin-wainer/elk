@article{silva_slugstochastically_2012,
	title = {{SLUG}—{STOCHASTICALLY} {LIGHTING} {UP} {GALAXIES}. {I}. {METHODS} {AND} {VALIDATING} {TESTS}},
	volume = {745},
	issn = {0004-637X},
	url = {https://doi.org/10.1088%2F0004-637x%2F745%2F2%2F145},
	doi = {10.1088/0004-637X/745/2/145},
	abstract = {The effects of stochasticity on the luminosities of stellar populations are an often neglected but crucial element for understanding populations in the low-mass or the low star formation rate regime. To address this issue, we present SLUG, a new code to “Stochastically Light Up Galaxies.” SLUG synthesizes stellar populations using a Monte Carlo technique that properly treats stochastic sampling including the effects of clustering, the stellar initial mass function, star formation history, stellar evolution, and cluster disruption. This code produces many useful outputs, including (1) catalogs of star clusters and their properties such as their stellar initial mass distributions and their photometric properties in a variety of filters, (2) two dimensional histograms of color-magnitude diagrams of every star in the simulation, and (3) the photometric properties of field stars and the integrated photometry of the entire simulated galaxy. After presenting the SLUG algorithm in detail, we validate the code through comparisons with STARBURST99 in the well-sampled regime, and with observed photometry of Milky Way clusters. Finally, we demonstrate SLUG's capabilities by presenting outputs in the stochastic regime. SLUG is publicly distributed through the Web site http://sites.google.com/site/runslug/.},
	language = {en},
	number = {2},
	urldate = {2020-08-07},
	journal = {The Astrophysical Journal},
	author = {Silva, Robert L. da and Fumagalli, Michele and Krumholz, Mark},
	month = jan,
	year = {2012},
	note = {Publisher: IOP Publishing},
	pages = {145},
	file = {IOP Full Text PDF:/Users/Tobin/Zotero/storage/IUVKW5BA/Silva et al. - 2012 - SLUG—STOCHASTICALLY LIGHTING UP GALAXIES. I. METHO.pdf:application/pdf},
}

@article{zackrisson_hubble_2011,
	title = {The {Hubble} {Space} {Telescope} colours of high-redshift {Population} {III} galaxies with strong {Lyα} emission},
	volume = {418},
	url = {http://adsabs.harvard.edu/abs/2011MNRAS.418L.104Z},
	doi = {10.1111/j.1745-3933.2011.01153.x},
	abstract = {Population III (pop III) galaxies, made partly or exclusively of 
metal-free stars, are predicted to exist at high redshifts and may
produce very strong Lyα emission. A substantial fraction of these
Lyα photons are likely absorbed in the intergalactic medium at z
{\textgreater} 6, but recent simulations suggest that significant Lyα
emission may be detectable up to z≈ 8.5, i.e. well into the
reionization epoch. Here, we argue that high-redshift pop III galaxies
with strong Lyα emission can be identified in Hubble Space
Telescope imaging data because of their unusual colours. We quantify
this effect in some of the filters used in Y-band dropout searches for
galaxies at z≈ 8 and find that pop III galaxies with high Lyα
fluxes may exhibit much bluer J-H colours at z≈ 8-10 than any normal
type of galaxy at these redshifts. This colour signature can arise even
if pop III stars account for as little as ∼10-3 to
10-2 of the stellar mass in these galaxies. Some of the
anomalously blue objects reported in current Y-band dropout samples do
in fact meet the colour criteria for Lyα-emitting pop III
galaxies.},
	urldate = {2020-08-07},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Zackrisson, Erik and Inoue, Akio K. and Rydberg, Claes-Erik and Duval, Florent},
	month = nov,
	year = {2011},
	keywords = {dark ages, first stars, galaxies: high-redshift, reionization, stars: Population III},
	pages = {L104--L108},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/N6BKFX7B/Zackrisson et al. - 2011 - The Hubble Space Telescope colours of high-redshif.pdf:application/pdf},
}

