	author = {Poovelil, Vijith Jacob and Zasowski, G. and Hasselquist, S. and Seth, A. and Donor, John and Beaton, Rachael L. and Cunha, K. and Frinchaboy, Peter M. and García-Hernández, D. A. and Hawkins, K. and Kratter, K. M. and Lane, Richard R. and Nitschelm, C.},
