	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics, Astrophysics - Solar and Stellar Astrophysics, distance scale, galaxies: distances and redshifts, stars: variables: Cepheids, cosmology: observations},
	pages = {10},
