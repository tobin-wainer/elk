	doi = {10.1007/978-94-007-5612-0_4},
	note = {Pages: 115
Publication Title: Planets, Stars and Stellar Systems. Volume 5: Galactic Structure and Stellar Populations
ADS Bibcode: 2013pss5.book..115K},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, Physics},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/YXDAD8BC/Kroupa et al. - 2013 - The Stellar and Sub-Stellar Initial Mass Function .pdf:application/pdf},
}

@article{lequeux_quantitative_1979,
	title = {A quantitative study of the upper {HR} diagram and a new determination of the local initial mass function.},
