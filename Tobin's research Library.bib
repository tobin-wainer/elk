	pages = {3348--3358},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/MJ2W4ZEZ/Weidner et al. - 2014 - Sampling methods for stellar masses and the mmax-M.pdf:application/pdf},
}

@article{calzetti_method_2010,
	title = {A {Method} for {Measuring} {Variations} in the {Stellar} {Initial} {Mass} {Function}},
