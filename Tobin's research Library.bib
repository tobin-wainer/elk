	pages = {5216--5237},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/RC8NC2MK/Giacobbe et al. - 2020 - Photometric rotation periods for 107 M dwarfs from.pdf:application/pdf;Snapshot:/Users/Tobin/Zotero/storage/KNFWUPCM/abstract.html:text/html},
