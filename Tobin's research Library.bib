}

@article{hoffmann_optical_2016,
	title = {Optical {Identification} of {Cepheids} in 19 {Host} {Galaxies} of {Type} {Ia} {Supernovae} and {NGC} 4258 with the {Hubble} {Space} {Telescope}},
