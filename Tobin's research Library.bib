	file = {Williams - 2003 - The Recent Star Formation History of the M31 Disk.pdf:/Users/Tobin/Zotero/storage/FRUSAAAC/Williams - 2003 - The Recent Star Formation History of the M31 Disk.pdf:application/pdf},
}

