	author = {Christy, C. T. and Jayasinghe, T. and Stanek, K. Z. and Kochanek, C. S. and Thompson, T. A. and Shappee, B. J. and Holoien, T. W. -S. and Prieto, J. L. and Dong, Subo and Giles, W.},
	month = may,
	year = {2022},
	note = {ADS Bibcode: 2022arXiv220502239C
Type: article},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/7QDVGJ4R/Christy et al. - 2022 - The ASAS-SN Catalog of Variable Stars X Discovery.pdf:application/pdf},
