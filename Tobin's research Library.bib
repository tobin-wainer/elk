	journal = {The Astrophysical Journal},
	author = {Baldry, Ivan K. and Glazebrook, Karl and Brinkmann, Jon and Ivezić, Željko and Lupton, Robert H. and Nichol, Robert C. and Szalay, Alexander S.},
	month = jan,
