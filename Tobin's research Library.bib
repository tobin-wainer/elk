@article{kovacs_empirical_2001,
	title = {Empirical relations for cluster {RR} {Lyrae} stars revisited},
	volume = {371},
	issn = {0004-6361, 1432-0746},
	url = {http://arxiv.org/abs/astro-ph/0103188},
	doi = {10.1051/0004-6361:20010346},
	abstract = {Our former study on the empirical relations between the Fourier parameters of the light curves of the fundamental mode RR Lyrae stars and their basic stellar parameters has been extended to considerably larger data sets. The most significant contribution to the absolute magnitude M\_v comes from the period P and from the first Fourier amplitude A\_1, but there are statistically significant contributions also from additional higher order components, most importantly from A\_3 and in a lesser degree from the Fourier phase phi\_51. When different colors are combined in reddening-free quantities, we obtain basically period-luminosity-color relations. Due to the log T\_eff (B-V, log g, [Fe/H]) relation from stellar atmosphere models, we would expect some dependence also on phi\_31. Unfortunately, the data are still not extensive and accurate enough to decipher clearly the small effect of this Fourier phase. However, with the aid of more accurate multicolor data on field variables, we show that this Fourier phase should be present either in V-I or in B-V or in both. From the standard deviations of the various regressions, an upper limit can be obtained on the overall inhomogeneity of the reddening in the individual clusters. This yields sigma\_E(B-V)\vphantom{\{}\}{\textless} 0.012 mag, which also implies an average minimum observational error of sigma\_V {\textgreater} 0.018 mag.},
	number = {2},
	urldate = {2022-06-20},
	journal = {Astronomy \& Astrophysics},
	author = {Kovacs, G. and Walker, A. R.},
	month = may,
	year = {2001},
	note = {arXiv:astro-ph/0103188},
	keywords = {Astrophysics},
	pages = {579--591},
	file = {arXiv Fulltext PDF:/Users/Tobin/Zotero/storage/EL4N52N9/Kovacs and Walker - 2001 - Empirical relations for cluster RR Lyrae stars rev.pdf:application/pdf;arXiv.org Snapshot:/Users/Tobin/Zotero/storage/MBTVBT8K/0103188.html:text/html},
}

@article{audenaert_tess_2021,
	title = {{TESS} {Data} for {Asteroseismology} ({T}'{DA}) {Stellar} {Variability} {Classification} {Pipeline}: {Set}-{Up} and {Application} to the {Kepler} {Q9} {Data}},
	volume = {162},
	issn = {0004-6256, 1538-3881},
