@article{lieberz_origin_2017,
	title = {On the origin of the {Schechter}-like mass function of young star clusters in disc galaxies},
	volume = {465},
	issn = {0035-8711},
	url = {https://ui.adsabs.harvard.edu/abs/2017MNRAS.465.3775L},
	doi = {10.1093/mnras/stw2953},
	abstract = {The mass function of freshly formed star clusters is empirically often described as a power law. However, the cluster mass function of populations of young clusters over the scale of a galaxy has been found to be described by a Schechter-function. Here we address this apparent discrepancy. We assume that in an annulus of an isolated self-regulated radially exponential axially symmetric disc galaxy, the local mass function of very young (embedded) clusters is a power law with an upper mass limit which depends on the local star formation rate density. Radial integration of this mass function yields a galaxy-wide embedded cluster mass function. This integrated embedded cluster mass function has a Schechter-type form, which results from the addition of many low-mass clusters forming at all galactocentric distances and rarer massive clusters only forming close to the centre of the galaxy.},
	urldate = {2022-01-19},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Lieberz, P. and Kroupa, P.},
	month = mar,
	year = {2017},
	note = {ADS Bibcode: 2017MNRAS.465.3775L},
	keywords = {stars: formation, galaxies: star clusters: general, galaxies: stellar content, mass function, stars: luminosity function, stars: statistics},
	pages = {3775--3783},
	file = {Full Text PDF:/Users/Tobin/Zotero/storage/CXT9LF9F/Lieberz and Kroupa - 2017 - On the origin of the Schechter-like mass function .pdf:application/pdf},
}

